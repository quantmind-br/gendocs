# Gendocs Go Configuration Example
# Copy this file to .env and fill in your values

# =============================================================================
# LLM Configuration - Analyzer
# =============================================================================
ANALYZER_LLM_PROVIDER=openai
ANALYZER_LLM_MODEL=gpt-4o
ANALYZER_LLM_API_KEY=sk-your-api-key-here
ANALYZER_LLM_BASE_URL=https://api.openai.com/v1
ANALYZER_AGENT_RETRIES=2
ANALYZER_LLM_TIMEOUT=180
ANALYZER_LLM_MAX_TOKENS=8192
ANALYZER_LLM_TEMPERATURE=0.0
ANALYZER_MAX_WORKERS=0

# =============================================================================
# LLM Configuration - Documenter
# =============================================================================
DOCUMENTER_LLM_PROVIDER=openai
DOCUMENTER_LLM_MODEL=gpt-4o
DOCUMENTER_LLM_API_KEY=sk-your-api-key-here
DOCUMENTER_LLM_BASE_URL=https://api.openai.com/v1
DOCUMENTER_AGENT_RETRIES=2
DOCUMENTER_LLM_TIMEOUT=180
DOCUMENTER_LLM_MAX_TOKENS=8192
DOCUMENTER_LLM_TEMPERATURE=0.0

# =============================================================================
# LLM Configuration - AI Rules Generator
# =============================================================================
AI_RULES_LLM_PROVIDER=openai
AI_RULES_LLM_MODEL=gpt-4o
AI_RULES_LLM_API_KEY=sk-your-api-key-here
AI_RULES_LLM_BASE_URL=https://api.openai.com/v1
AI_RULES_AGENT_RETRIES=2
AI_RULES_LLM_TIMEOUT=240
AI_RULES_LLM_MAX_TOKENS_MARKDOWN=8192
AI_RULES_LLM_MAX_TOKENS_CURSOR=16384
AI_RULES_LLM_TEMPERATURE=0.0

# =============================================================================
# Gemini Configuration (for Google Gemini provider)
# =============================================================================
GEMINI_USE_VERTEX_AI=false
GEMINI_PROJECT_ID=your-project-id
GEMINI_LOCATION=us-central1

# =============================================================================
# GitLab Configuration
# =============================================================================
GITLAB_API_URL=https://gitlab.example.com
GITLAB_USER_NAME=AI Analyzer
GITLAB_USER_USERNAME=agent_doc
GITLAB_USER_EMAIL=agent@example.com
GITLAB_OAUTH_TOKEN=glpat-your-token-here

# =============================================================================
# HTTP Retry Configuration
# =============================================================================
HTTP_RETRY_MAX_ATTEMPTS=5
HTTP_RETRY_MULTIPLIER=1
HTTP_RETRY_MAX_WAIT_PER_ATTEMPT=60
HTTP_RETRY_MAX_TOTAL_WAIT=300

# =============================================================================
# Logging Configuration
# =============================================================================
CONSOLE_LOG_LEVEL=DEBUG
FILE_LOG_LEVEL=INFO

# =============================================================================
# Observability (Optional)
# =============================================================================
ENABLE_LANGFUSE=false
LANGFUSE_PUBLIC_KEY=pk-xxx
LANGFUSE_SECRET_KEY=sk-xxx
